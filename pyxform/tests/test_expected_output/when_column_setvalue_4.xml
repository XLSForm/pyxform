<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>when_column_setvalue_4</h:title>
    <model odk:xforms-version="1.0.0">
      <instance>
        <when_column_setvalue_4 id="when_column_setvalue_4">
          <a/>
          <d/>
          <meta>
            <instanceID/>
          </meta>
        </when_column_setvalue_4>
      </instance>
      <bind nodeset="/when_column_setvalue_4/a" type="string"/>
      <bind nodeset="/when_column_setvalue_4/d" type="dateTime"/>
      <bind jr:preload="uid" nodeset="/when_column_setvalue_4/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/when_column_setvalue_4/a">
      <label>Enter text</label>
      <setvalue event="xforms-value-changed" ref=" /when_column_setvalue_4/d " value=""/>
    </input>
    <input ref="/when_column_setvalue_4/d">
      <label>Date of something</label>
    </input>
  </h:body>
</h:html>
