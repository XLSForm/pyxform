<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>badcalc</h:title>
    <model>
      <instance>
        <badcalc id="badcalc">
          <badcalc/>
          <meta>
            <instanceID/>
          </meta>
        </badcalc>
      </instance>
      <bind calculate="because pyxform doesn't know what to do" nodeset="/badcalc/badcalc" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/badcalc/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body/>
</h:html>
